ll,"code":["let firstResponse: (OpenAPI.Response.StatusCode, Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>)","firstResponse = operation.responses[0]!","","\/\/ literally documented as \"200\" status code:","let successResponse: Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>","successResponse = operation.responses[status: 200]!","","\/\/ documented as \"2XX\" status code:","let successResponse2: Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>","successResponse2 = operation.responses[.range(.success)]!"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If you want to access the response (assuming it is inlined) you need to grab"},{"type":"text","text":" "},{"type":"text","text":"it out of the "},{"type":"codeVoice","code":"Either"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Example:"}]}]},{"type":"codeListing","syntax":null,"code":["let inlinedResponse = successResponse.responseValue"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can also look the response up in the "},{"type":"codeVoice","code":"Components"},{"type":"text","text":". For convenience, you"},{"type":"text","text":" "},{"type":"text","text":"can ask to have the "},{"type":"codeVoice","code":"Either"},{"type":"text","text":" looked up and the result will be the "},{"type":"codeVoice","code":"Response"},{"type":"text","text":" "},{"type":"text","text":"regardless of whether the "},{"type":"codeVoice","code":"Response"},{"type":"text","text":" was inlined or found in the "},{"type":"codeVoice","code":"Components"},{"type":"text","text":"."}]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"Example:"}]}]},{"type":"codeListing","syntax":null,"code":["let foundResponse: OpenAPI.Response","foundResponse = document.components.lookup(successResponse)!"]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections