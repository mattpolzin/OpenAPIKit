tifier","identifier":"doc:\/\/OpenAPIKit\/documentation\/OpenAPIKit\/OpenAPI\/Response\/Map"}],"languages":["swift"]}]},{"kind":"content","content":[{"text":"Discussion","type":"heading","level":2,"anchor":"discussion"},{"inlineContent":[{"text":"The status code keys can be integer values, ranges, or even the","type":"text"},{"text":" ","type":"text"},{"type":"codeVoice","code":"default"},{"text":" which just refers to the response to expect where no","type":"text"},{"text":" ","type":"text"},{"text":"other respones apply.","type":"text"}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"Because the map is ordered, you can access responses by either"},{"type":"text","text":" "},{"type":"text","text":"status code or index. Notice that the values of this dictionary are actually"},{"type":"text","text":" "},{"type":"codeVoice","code":"Either"},{"type":"text","text":" an inline "},{"type":"codeVoice","code":"Response"},{"type":"text","text":" or a reference to a "},{"type":"codeVoice","code":"Response"},{"type":"text","text":" that is"},{"type":"text","text":" "},{"type":"text","text":"defined elsewhere."}],"type":"paragraph"},{"inlineContent":[{"type":"strong","inlineContent":[{"text":"Example:","type":"text"}]}],"type":"paragraph"},{"syntax":null,"type":"codeListing","code":["let firstResponse: (OpenAPI.Response.StatusCode, Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>)","firstResponse = operation.responses[0]!","","\/\/ literally documented as \"200\" status code:","let successResponse: Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>","successResponse = operation.responses[status: 200]!","","\/\/ documented as \"2XX\" status code:","let successResponse2: Either<OpenAPI.Reference<OpenAPI.Response>, OpenAPI.Response>","successResponse2 = operation.responses[.range(.success)]!"]},{"inlineContent":[{"type":"text","text":"If you want to access the response (assuming it is inlined) you need to grab"},{"type":"text","text":" "},{"type":"text","text":"it out of the "},{"type":"codeVoice","code":"Either"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"type":"text","text":"Example:"}],"type":"strong"}],"type":"paragraph"},{"syntax":null,"type":"codeListing","code":["let inlinedResponse = successResponse.responseValue"]},{"inlineContent":[{"type":"text","text":"You can also look the response up in the "},{"code":"Components","type":"codeVoice"},{"type":"text","text":". For convenience, you"},{"type":"text","text":" "},{"type":"text","text":"can ask to have the "},{"code":"Either","type":"codeVoice"},{"type":"text","text":" looked up and the result will be the "},{"code":"Response","type":"codeVoice"},{"type":"text","text":" "},{"type":"text","text":"regardless of whether the "},{"code":"Response","type":"codeVoice"},{"type":"text","text":" was inlined or found in the "},{"code":"Components","type":"codeVoice"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"inlineContent":[{"text":"Example:","type":"text"}],"type":"strong"}],"type":"paragraph"},{"syntax":null,"type":"codeListing","code":["let foundResponse: OpenAPI.Response","foundResponse = document.components.lookup(successResponse)!"]}]}],"sections":[],"abstract":[{"text":"The possible responses for this operation, keyed by status code.","type":"text"}],"references":{"doc://OpenAPIKit/documentation/OpenAPIKit/OpenAPI/Response/Map":{"navigatorTitle":[{"kind":"identifier","text":"Map"}],"title":"OpenAPI.Response.Map","role":"symbol","url":"\/documentation\/openapikit\/openapi\/response\/map","abstract":[],"type":"topic","identifier":"doc:\/\/OpenAPIKit\/documentation\/OpenAPIKit\/OpenAPI\/Response\/Map","fragments":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"kind":"identifier","text":"Map"}],"kind":"symbol"},"doc://OpenAPIKit/documentation/OpenAPIKit/OpenAPI":{"type":"topic","kind":"symbol","navigatorTitle":[{"kind":"identifier","text":"OpenAPI"}],"title":"OpenAPI"