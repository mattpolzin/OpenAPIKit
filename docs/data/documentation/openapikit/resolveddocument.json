at the"},{"type":"text","text":" "},{"type":"codeVoice","code":"OpenAPI.Operation"},{"type":"text","text":" representing the endpoint, the "},{"type":"codeVoice","code":"OpenAPI.PathItem"},{"type":"text","text":" "},{"type":"text","text":"representing the route containing the endpoint, and you would need to potentially"},{"type":"text","text":" "},{"type":"text","text":"resolve references to the "},{"type":"codeVoice","code":"OpenAPI.Components"},{"type":"text","text":"."}],"type":"paragraph"},{"inlineContent":[{"type":"text","text":"With "},{"type":"codeVoice","code":"ResolvedDocument"},{"type":"text","text":", the same task is much simpler:"}],"type":"paragraph"},{"type":"codeListing","syntax":null,"code":["let document: OpenAPI.Document = ...","","let resolvedDocument = try document","    .locallyDereferenced()","    .resolved()","","let endpoint = resolvedDocument","    .routesByPath[\"\/example\/path\"]!.patch!","","\/\/ This will be all parameters defined anywhere that","\/\/ apply to this endpoint:","let parameters = endpoint.parameters"]}]}],"identifier":{"url":"doc:\/\/OpenAPIKit\/documentation\/OpenAPIKit\/ResolvedDocument","interfaceLanguage":"swift"},"references":{"doc://OpenAPIKit/documentation/OpenAPIKit/ResolvedDocument/underlyingDocument":{"kind":"symbol","identifier":"doc:\/\/OpenAPIKit\/documentation\/OpenAPIKit\/ResolvedDocument\/underlyingDocument","title":"underlyingDocument","url":"\/documentation\/openapikit\/resolveddocument\/underlyingdocument","abstract":[{"text":"Access the underlying dereferenced document.","type":"text"}],"fragments":[{"text":"let","kind":"keyword"},{"text":" ","kind":"text"},{"text":"underlyingDocument","kind":"identifier"},{"text":": ","kind":"text"},{"preciseIdentifier":"s:10OpenAPIKit20DereferencedDocumentV","text":"DereferencedDocument","kind":"typeIdentifier"}],"type":"topic","role":"symbol"},"doc://OpenAPIKit/documentation/OpenAPIKit/ResolvedDocument":{"title":"ResolvedDocument","kind":"symbol","navigatorTitle":[{